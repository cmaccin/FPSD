@model IEnumerable<FPSD.Models.Standard>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StandardsType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StandardsName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StandardsDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StandardsType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StandardsName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StandardsDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StandardsID }) |
            @Html.ActionLink("Details", "Details", new { id=item.StandardsID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StandardsID })
        </td>
    </tr>
}

</table>
